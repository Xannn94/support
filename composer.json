{
    "name":        "xannn94/support",
    "description": "xannn94 Support Helpers",
    "keywords":    ["xannn94", "arcanesoft", "support", "laravel"],
    "authors": [
        {
            "name":     "xannn94",
            "email":    "axiles94@gmail.com"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                   ">=5.6.4",
        "illuminate/filesystem": "~5.0",
        "illuminate/support":    "~5.0"
    },
    "require-dev": {
        "phpunit/phpcov":  "~3.0",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Xannn94\\Support\\": "src/"
        },
        "files": ["constants.php", "helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Xannn94\\Support\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
